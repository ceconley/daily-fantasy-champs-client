<div class="container" "table-responsive" style="margin-top: 50px;">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th scope="col">Player</th>
        <th scope="col">Lineup</th>
        <th scope="col">Score</th>
        <th scope="col">Rank</th>
      </tr>
    </thead>
    <tbody>
      {{#if contest}}
      <tr>
        <td>{{contest.contest.user.email}}</td>
        <td>{{contest.contest.user.lineup}}</td>
        <td>{{contest.contest.user.lineup.score}}</td>
        <td>{{contest.contest.user.lineup.rank}}</td>
      </tr>
      {{/if}}
    </tbody>
  </table>
  <button id="join-contest" data-dismiss="modal">Join</button>
</div>