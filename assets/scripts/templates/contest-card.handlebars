<div class="container" "table-responsive" style="margin-top: 50px;">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th scope="col">Player</th>
        <th scope="col">Lineup</th>
        <th scope="col">Score</th>
        <th scope="col">Rank</th>
      </tr>
    </thead>
    <tbody>
      {{#each entries as |entry|}}
      <tr>
        <td>{{this.user.email}}</td>
        <td class="table-responsive">
          <table class="table-bordered table-in-table">
            <tr>
              <td>{{this.lineup.qb}}</td>
              <td>{{this.lineup.wr1}}</td>
              <td>{{this.lineup.te}}</td>
            </tr>
            <tr>
              <td>{{this.lineup.rb1}}</td>
              <td>{{this.lineup.wr2}}</td>
              <td>{{this.lineup.flex}}</td>
            </tr>
            <tr>
              <td>{{this.lineup.rb2}}</td>
              <td>{{this.lineup.wr3}}</td>
              <td>{{this.lineup.dst}}</td>
            </tr>
          </table>
        </td>
        <td>{{this.lineup.score}}</td>
        <td>{{this.lineup.rank}}</td>
      {{/each}}
      </tr>
    </tbody>
  </table>
  <button id="join-contest" data-dismiss="modal">Join</button>
  <p id="contest-card-message"></p>
</div>