<div class="container" "table-responsive" style="margin-top: 50px;">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th scope="col">Player</th>
        <th scope="col">Lineup</th>
        <th scope="col">Score</th>
        <th scope="col">Rank</th>
      </tr>
    </thead>
    <tbody>
      {{#each entries as |entry|}}
      <tr>
        <td>{{this.user.email}}</td>
        <td>{{this.lineup.qb}}, {{this.lineup.rb1}}, {{this.lineup.rb2}}, {{this.lineup.wr1}}, {{this.lineup.wr2}}, {{this.lineup.wr3}}, {{this.lineup.te}}, {{this.lineup.flex}}, {{this.lineup.dst}}</td>
        <td>{{this.lineup.score}}</td>
        <td>{{this.lineup.rank}}</td>
      {{/each}}
      </tr>
    </tbody>
  </table>
  <button id="join-contest" data-dismiss="modal">Join</button>
  <p id="contest-card-message"></p>
</div>