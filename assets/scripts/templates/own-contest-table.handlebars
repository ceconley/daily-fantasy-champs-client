<div class="container table-responsive">
  <table id="contest" class="table table-striped table-bordered">
    <thead>
      <tr id="header-row">
        <th scope="col">Name</th>
        <th scope="col">Lineup</th>
        <th scope="col">Entrants</th>
        <th scope="col">Leader</th>
      </tr>
    </thead>
    <br/>
    <br/>
    <tbody>
      {{#each entries as |entries|}}
      <tr class="contest-row">
        <td class="contest-name" id="{{this.contest.id}}" data-entry-id="{{this.id}}" data-toggle="modal" data-target="#modalContestCard">{{this.contest.name}}</td>
        <td>
          <table class="table-bordered">
            <tr>
              <td>{{this.lineup.qb}}</td>
              <td>{{this.lineup.wr1}}</td>
              <td>{{this.lineup.te}}</td>
            </tr>
            <tr>
              <td>{{this.lineup.rb1}}</td>
              <td>{{this.lineup.wr2}}</td>
              <td>{{this.lineup.flex}}</td>
            </tr>
            <tr>
              <td>{{this.lineup.rb2}}</td>
              <td>{{this.lineup.wr3}}</td>
              <td>{{this.lineup.dst}}</td>
            </tr>
          </table>
        </td>
        <td>{{this.contest.entrants_current}}/{{this.contest.entrants_total}}</td>
        <td>{{this.contest.winner}}</td>
      {{/each}}
      </tr>
    </tbody>
  </table>
</div>